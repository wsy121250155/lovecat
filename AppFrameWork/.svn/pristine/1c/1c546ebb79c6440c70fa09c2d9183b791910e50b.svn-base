package com.mobile.base.util;

public class AppConfig {
	public static String APPID = "nju";	
	private static boolean  isUseCard = true; //是否使用SD卡
	private static String TEMPPATH =  "zsdx_temp"; //缓存目录
	private static boolean isShowLog = true;
	

	public static synchronized boolean isUseCard() {
		return isUseCard;
	}

	public static synchronized void setUseCard(boolean isUseCard) {
		AppConfig.isUseCard = isUseCard;
	}

	public static synchronized String getTEMPPATH() {
		return TEMPPATH;
	}

	public static synchronized void setTEMPPATH(String tEMPPATH) {
		TEMPPATH = tEMPPATH;
	}

	public static synchronized String getAPPID() {
		return APPID;
	}

	public static synchronized void setAPPID(String aPPID) {
		APPID = aPPID;
	}

	public static synchronized boolean isShowLog() {
		return isShowLog;
	}

	public static synchronized void setShowLog(boolean isShowLog) {
		AppConfig.isShowLog = isShowLog;
	}
	
	
}
